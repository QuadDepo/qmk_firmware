{
    "configurations": [
        {
            "name": "equinox",
            "includePath": [
                "${workspaceFolder}/**",
                "${env:GCC_ARM}/arm-none-eabi/include",
                "${env:NRF_SDK_KBD}/**",
                "${env:QMK_ROOT}/**"
            ],
            "defines": [
                "CONFIG_GPIO_AS_PINRESET",
                "INITIALIZE_USER_SECTIONS",
                "FLOAT_ABI_HARD",
                "SOFTDEVICE_PRESENT",
                "NRF52",
                "NRF52840_XXAA",
                "NRF_SD_BLE_API_VERSION=6",
                "CONFIG_NFCT_PINS_AS_GPIOS",
                "SWI_DISABLE0",
                "__CC_ARM"
            ],

            "compilerPath": "${env:GCC_ARM}/bin/arm-none-eabi-gcc",
            "cStandard": "c11",
            "cppStandard": "c++17",
            "intelliSenseMode": "clang-x64"
        }
    ],
    "version": 4
}